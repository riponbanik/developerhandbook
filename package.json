{
  "name": "developerhandbook",
  "version": "2.27.1",
  "description": "Source code for DeveloperHandbook.com blog",
  "main": "index.js",
  "scripts": {
    "start": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "bump": "bump --prompt",
    "build": "rimraf .cache public && gatsby build",
    "build:noclean": "gatsby build",
    "deploy": "npm run bump -- --prompt --tag --push --all && npm run build && s3-deploy './public/**' --cwd './public/' --bucket developerhandbook.com  --deleteRemoved --gzip && npm run clear",
    "clear": "aws-cloudfront-invalidate EI5Q6NGOHKSZ4",
    "prettier": "prettier \"./src/**/*.{js,json,yaml,yml}\" --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpreecedev/developerhandbook.git"
  },
  "devDependencies": {
    "aws-cloudfront-invalidate": "^0.1.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "gatsby-plugin-canonical-urls": "^2.0.12",
    "gatsby-plugin-no-sourcemaps": "^2.0.2",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sitemap": "^2.0.11",
    "prettier": "^1.16.3",
    "rimraf": "^2.6.3",
    "s3-deploy": "^1.3.0",
    "stylelint": "^9.10.1",
    "stylelint-config-rational-order": "^0.0.4",
    "version-bump-prompt": "^4.3.0"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "core-js": "^2.6.5",
    "disqus-react": "^1.0.5",
    "gatsby": "^2.3.12",
    "gatsby-image": "^2.0.37",
    "gatsby-plugin-feed": "^2.1.0",
    "gatsby-plugin-google-analytics": "^2.0.18",
    "gatsby-plugin-manifest": "^2.0.26",
    "gatsby-plugin-polyfill-io": "^1.1.0",
    "gatsby-plugin-postcss": "^2.0.7",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-remark-autolink-headers": "^2.0.16",
    "gatsby-remark-copy-linked-files": "^2.0.11",
    "gatsby-remark-images": "^3.0.10",
    "gatsby-remark-prismjs": "^3.2.7",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.1.17",
    "github-slugger": "^1.2.1",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-reporter": "^6.0.1",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "blog",
    "development",
    "c#",
    ".net",
    "code",
    "javascript",
    "react"
  ],
  "author": "Jon Preece",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jpreecedev/developerhandbook/issues"
  },
  "homepage": "https://github.com/jpreecedev/developerhandbook#readme",
  "browserslist": [
    "defaults",
    "IE >= 11",
    "firefox >= 43"
  ]
}
