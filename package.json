    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.7.0",
    "gatsby-plugin-canonical-urls": "^1.0.18",
    "gatsby-plugin-sass": "^1.0.26",
    "gatsby-plugin-sitemap": "^1.2.25",
    "prettier": "^1.12.0",
    "stylelint": "^9.5.0",
    "stylelint-config-rational-order": "^0.0.2"
